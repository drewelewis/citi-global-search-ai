  "version": "0.2.0",
  "configurations": [
    {
        "type": "java",
        "name": "Attach",
        "request": "attach",
        "hostName": "localhost",
        "port": "<debug port of the debuggee>"
    },
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "Debugger: Main",
      "type": "debugpy",
      "request": "launch",
      "program": "main.py",
      "console": "integratedTerminal"
    }
    ,
    {
      "name": "Debugger: Main RAG",
      "type": "debugpy",
      "request": "launch",
      "program": "main_rag.py",
      "console": "integratedTerminal"

    }
  ]
}
